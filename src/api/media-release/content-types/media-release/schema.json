{
  "kind": "collectionType",
  "collectionName": "media_releases",
  "info": {
    "singularName": "media-release",
    "pluralName": "media-releases",
    "displayName": "Media Release",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {}
    },
    "country": {
      "type": "enumeration",
      "enum": [
        "Global",
        "Malaysia",
        "Bangladesh",
        "Sri Lanka",
        "Pakistan",
        "Cambodia",
        "Myanmar",
        "Laos",
        "Philippines",
        "Indonesia"
      ],
      "required": true,
      "pluginOptions": {}
    },
    "content": {
      "type": "richtext",
      "required": true,
      "pluginOptions": {}
    },
    "cover_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {}
    },
    "duration": {
      "type": "integer",
      "required": true,
      "pluginOptions": {}
    },
    "published_date": {
      "type": "date",
      "required": true
    }
  }
}
